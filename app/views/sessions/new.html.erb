<div class="container" id="new-session">
  <div class="thumbnail">
    <h1>Sign In</h1>
    <br>
    <h4>Enter your email and password.</h4>

    <%= form_tag do %>
      <div class="form-group">
        <!-- <%= label_tag :email %> -->
        <%= email_field_tag :email, placeHolder="you@domain.com" %>
      </div>

      <div class="form-group">
        <!-- <%= label_tag :password %> -->
        <%= password_field_tag :password, placeHolder="password" %>
      </div>

      <div class="form-group">
        <%= submit_tag "Sign In", class: "btn btn-default"%>
        <div class="">
          <h4><%= link_to 'Sign Up', new_user_path %></h4>
        </div>
      </div>
    <% end %>
  </div>
</div>
